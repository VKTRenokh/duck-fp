{
  "semi": false,
  "trailingComma": "all",
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 60,
  "bracketSpacing": true,
  "importOrder": [
    "^-->$",
    "^->t/(.*)$",
    "^->/(.*)$",
    "^[./]"
  ],
  "importOrderSortSpecifiers": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ]
}
