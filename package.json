{
  "name": "@victorenokh/maybe.ts",
  "version": "1.9.0",
  "description": "Maybe, either, state monad",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc --build",
    "release": "npm run build && changeset publish",
    "version-packages": "changeset version",
    "test-coverage": "jest --coverage",
    "test": "jest",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prepare": "husky",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./maybe": {
      "import": "./dist/maybe/index.js",
      "types": "./dist/maybe/index.d.ts"
    },
    "./either": {
      "import": "./dist/either/index.js",
      "types": "./dist/either/index.d.ts"
    },
    "./functions": {
      "import": "./dist/functions/index.js",
      "types": "./dist/functions/index.d.ts"
    },
    "./identity": {
      "import": "./dist/identity/index.js",
      "types": "./dist/identity/index.d.ts"
    },
    "./observable": {
      "import": "./dist/observable/index.js",
      "types": "./dist/observable/index.d.ts"
    },
    "./reader": {
      "import": "./dist/reader/index.js",
      "types": "./dist/reader/index.d.ts"
    },
    "./readerT": {
      "import": "./dist/readerT/index.js",
      "types": "./dist/readerT/index.d.ts"
    },
    "./state": {
      "import": "./dist/state/index.js",
      "types": "./dist/state/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vktrenokh/maybe.ts"
  },
  "bugs": {
    "url": "https://github.com/vktrenokh/maybe.ts/issues"
  },
  "keywords": [
    "fp",
    "ts",
    "monad",
    "maybe",
    "option",
    "either",
    "result",
    "observable",
    "monads",
    "readerT",
    "reader",
    "transformer"
  ],
  "author": "Victor Enokh Kuptsov <victorenokh@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
